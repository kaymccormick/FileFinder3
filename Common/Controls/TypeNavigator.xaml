<UserControl Name="userControl" x:Class="Common.Controls.TypeNavigator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Common.Controls"
             xmlns:appShared="clr-namespace:AppShared;assembly=AppShared"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d" 
             d:DesignHeight="180" d:DesignWidth="300">
    <DockPanel>
        <local:TypeControl Detailed="False" Target="{Binding ElementName=typeNavigationFrame}"  DockPanel.Dock="Top">
            <appShared:App.RenderedType><Binding ElementName="userControl" Path="(appShared:App.RenderedType)" FallbackValue="{x:Type system:String}"/></appShared:App.RenderedType>
        </local:TypeControl>
        <Frame Name="typeNavigationFrame" Source="DefaultPage.xaml"></Frame>
    </DockPanel>
</UserControl>
