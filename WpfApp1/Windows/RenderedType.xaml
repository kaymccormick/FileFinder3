<UserControl x:Class="WpfApp1.Windows.RenderedType"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp1.Windows"
             xmlns:controls="clr-namespace:WpfApp1.Controls"
             xmlns:menus="clr-namespace:WpfApp1.Menus"
             xmlns:commands="clr-namespace:WpfApp1.Commands"
             xmlns:appShared="clr-namespace:AppShared;assembly=AppShared"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="150">
    <UserControl.CommandBindings><CommandBinding 
                                                 CanExecute="CommandBinding_OnCanExecute"
                                                 /></UserControl.CommandBindings>
    <Grid>
            <controls:TypeControl>
        <appShared:AppProperties.RenderedType><x:Type TypeName="menus:MenuItemList"/></appShared:AppProperties.RenderedType>
                </controls:TypeControl>
            
    </Grid>
</UserControl>
